    "{\n",
    " \"cells\": [\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Moca Only Dataset Analysis\\n\",\n",
    "    \"\\n\",\n",
    "    \"This notebook focuses on extending the MOCA-only dataset with new columns and performing a multicollinearity analysis. It also prepares the dataset for use with the correlation_EFA notebook.\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Import necessary libraries\\n\",\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"from pathlib import Path\"\n",
    "   ],\n",
    "   \"execution_count\": null,\n",
    "   \"outputs\": []\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Define paths and parameters\\n\",\n",
    "    \"OUT_DIR = '../../outputs/uds_extraction'\\n\",\n",
    "    \"moca_only_path = Path(OUT_DIR) / 'moca_only.parquet'\\n\",\n",
    "    \"new_columns = ['DIGFORCT', 'DIGFORSL', 'DIGBACCT', 'DIGBACLS', 'MINTTOTS', 'MINTTOTW', 'MINTSCNG', 'MINTSCNC', 'MINTPCNG', 'MINTPCNC']\\n\"\n",
    "   ],\n",
    "   \"execution_count\": null,\n",
    "   \"outputs\": []\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Load the MOCA-only dataset\\n\",\n",
    "    \"df_moca_only = pd.read_parquet(moca_only_path)\\n\"\n",
    "   ],\n",
    "   \"execution_count\": null,\n",
    "   \"outputs\": []\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Extend the dataset with new columns\\n\",\n",
    "    \"for col in new_columns:\\n\",\n",
    "    \"    df_moca_only[col] = np.nan  # Fill with NaN or any default value\\n\"\n",
    "   ],\n",
    "   \"execution_count\": null,\n",
    "   \"outputs\": []\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Save the extended dataset\\n\",\n",
    "    \"extended_moca_path = Path(OUT_DIR) / 'moca_extended.parquet'\\n\",\n",
    "    \"df_moca_only.to_parquet(extended_moca_path)\\n\"\n",
    "   ],\n",
    "   \"execution_count\": null,\n",
    "   \"outputs\": []\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Perform multicollinearity analysis\\n\",\n",
    "    \"from statsmodels.stats.outliers_influence import variance_inflation_factor\\n\",\n",
    "    \"\\n\",\n",
    "    \"def calculate_vif(df):\\n\",\n",
    "    \"    vif_data = pd.DataFrame()\\n\",\n",
    "    \"    vif_data['Feature'] = df.columns\\n\",\n",
    "    \"    vif_data['VIF'] = [variance_inflation_factor(df.values, i) for i in range(len(df.columns))]\\n\",\n",
    "    \"    return vif_data.sort_values(by='VIF', ascending=False)\\n\"\n",
    "   ],\n",
    "   \"execution_count\": null,\n",
    "   \"outputs\": []\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Calculate VIF for the extended dataset\\n\",\n",
    "    \"vif_df = calculate_vif(df_moca_only.drop(columns=['has_MOCA']))\\n\",\n",
    "    \"print(vif_df)\\n\"\n",
    "   ],\n",
    "   \"execution_count\": null,\n",
    "   \"outputs\": []\n",
    "  }\n",
